{
  "name": "price-feed",
  "config":
  {
    "connector.class": "PriceFeedConnector",
    "tasks.max": "1",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.converters.FloatConverter",
    "topic": "prices",
    "frequency": "2"
  }
}
